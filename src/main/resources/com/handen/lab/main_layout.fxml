<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.handen.lab.controller.MainController">
    <Canvas fx:id="canvas" height="400.0" width="514.0" />
    <VBox layoutX="524.0" prefHeight="400.0" prefWidth="184.0">
        <ComboBox fx:id="combobox" prefWidth="150.0" />
        <VBox fx:id="container" prefHeight="218.0" prefWidth="184.0">
            <Label fx:id="label1" />
            <TextField fx:id="textField1" />
            <Label fx:id="label2" />
            <TextField fx:id="textField2" />
            <Label fx:id="label3" />
            <TextField fx:id="textField3" />
            <Label fx:id="label4" />
            <TextField fx:id="textField4" />
            <Label fx:id="label5" />
            <TextField fx:id="textField5" />
            <Label fx:id="label6" />
            <TextField fx:id="textField6" />
        </VBox>
         <Label fx:id="errorLabel" prefHeight="34.0" prefWidth="183.0" textFill="RED" />
        <Button fx:id="drawButton" alignment="TOP_CENTER" mnemonicParsing="false" onMouseClicked="#OnDrawButtonClicked" text="Draw!" />
    </VBox>
</AnchorPane>
